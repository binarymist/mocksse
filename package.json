{
  "name": "mocksse",
  "version": "1.0.2",
  "description": "Allows testing of EventSource / Server Sent Events",
  "main": "src/mocksse.js",
  "scripts": {
    "lint": "eslint .",
    "deps": "npm-check",
    "update:deps": "npm-check -u",
    "test": "lab --assert @hapi/code --coverage",
    "test:debug": "node --inspect-brk ./node_modules/.bin/lab --assert @hapi/code --timeout 0",
    "pretest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/binarymist/mocksse.git"
  },
  "keywords": [
    "Server Sent Events",
    "SSE",
    "mock",
    "mocking",
    "test",
    "testing",
    "EventSource"
  ],
  "author": {
    "name": "BinaryMist",
    "email": "services@binarymist.net"
  },
  "dependencies": {},
  "devDependencies": {
    "@hapi/code": "^8.0.2",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "@hapi/lab": "^24.1.0",
    "npm-check": "^5.9.2",
    "pre-commit": "^1.2.2"
  },
  "license": "MIT",
  "bugs": "https://gitlab.com/binarymist/mocksse/issues"
}
