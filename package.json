    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "lab": "^15.5.0",
    "npm-check": "^5.7.1",
    "pre-commit": "^1.2.2",
    "rewire": "^4.0.1",
    "sinon": "^6.1.5"
  },
